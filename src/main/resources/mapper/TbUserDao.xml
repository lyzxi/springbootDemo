<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhangxi.dao.TbUserDao">
    <select id="getUserMobile" resultType="map">
        SELECT A.id, A.mobile FROM tb_user A WHERE A.id = #{byname}
    </select>
</mapper>